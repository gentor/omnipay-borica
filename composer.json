{
    "name": "gentor/omnipay-borica",
    "type": "library",
    "description": "Borica Way4 gateway for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "borica",
        "purchase"
    ],
    "homepage": "https://github.com/gentor/omnipay-borica",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeni Nurkov",
            "email": "evgeni.nurkov@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\Borica\\": "src/"
        }
    },
    "require": {
        "php": "^7",
        "omnipay/common": "^3",
        "ext-openssl": "*",
        "ext-json": "*",
        "ramsey/uuid": "^3 || ^4"
    },
    "require-dev": {
        "omnipay/tests": "^3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "prefer-stable": true
}
